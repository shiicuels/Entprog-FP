@model IEnumerable<MP_ENTPROG_CUELLOSM_PANGILINANJA.Models.Classlist>

@{
    ViewBag.Title = "ListStudent";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

<h2>Student's Classlist</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ClassDays)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Venue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Subject)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Faculty)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ClassDays)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Venue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Subject)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Faculty)
        </td>   
    </tr>
}

</table>
